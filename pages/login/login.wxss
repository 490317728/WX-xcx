page{
    width:100%;
    height:100%;
    display: flex;
    flex-direction: column;
    background-color: #f5f5f5;
}
.header{
    width:100%;
    height:360rpx;
}
.warp{
    width:60%;
    height:80rpx;
    margin:240rpx 20% 0;
    display: flex;
}
.warp text{
    flex: 1;
    height:80rpx;
    line-height:80rpx;
    font-size: 32rpx;
    border:2rpx solid #89be48;
    background-color: #fff;
    text-align: center;
    color:#89be48;
}
.warp text:nth-of-type(1){
    border-right:0;
}
.warp text:nth-of-type(2){
    border-left:0;
}
.warp .active{
    background-color: #89be48;
    color:#fff;
}
.swiper{
    flex: 1;
    width:100%;
}
.swiper>.swiper-item{
    width:100%;
    height:100%;
}
.mimaLogin,.phoneLogin{
    display: block;
    width:100%;
    height:100%;
}
.mimaLogin view,.phoneLogin view{
    width:100%;
    height:120rpx;
    background-color: #fff; 
}
.mimaLogin view label,.phoneLogin view label{
    float: left;
    width:200rpx;
    height:120rpx;
    line-height:120rpx;
    font-size: 34rpx;
    color:#333;
    text-indent: 20rpx;
}
.mimaLogin view input,.phoneLogin view input{
    float: left;
    height:120rpx;
    line-height:120rpx;
    text-indent: 10rpx;
    font-size: 30rpx;
}
.mimaLogin view:nth-of-type(1),.phoneLogin view:nth-of-type(1){
    border-bottom: 2rpx solid #ccc;
}
.mimaLogin button,.phoneLogin button{
    width:96%;
    height:100rpx;
    margin:20rpx 2%;
    border-radius: 0;
    border:0;
    color:#fff;
}
.mimaLogin button:nth-of-type(2){
    background-color: #fff;
    color:#89be48;
    border-radius: 0;
    border:2rpx solid #89be48;
}
.random_warp{
    position: relative;
}
.random{
    display: block;
    position: absolute;
    right:30rpx;
    top:20rpx;
    width:200rpx;
    height:80rpx;
    line-height:80rpx;
    text-align: center;
    background-color: #89be48;
    font-size: 30rpx;
    color: #fff;
}