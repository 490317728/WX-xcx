page {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  }
/*首页头部*/
.titleList{
  height: 100rpx;
}
.nav{
  width:1520rpx;
  height:100rpx;
}

.nav>view{
  width: 190rpx;
  float: left;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 36rpx;
  color:#999;
  box-sizing: border-box;
  border-bottom: 5rpx solid transparent;
}
.nav view.active{
  color:#89be48;
  border-bottom: 5rpx solid #89be48;
}
/*首页内容区*/
.swiper {
  width: 100%;
  flex: 1;
}
.swiper-item {
  width: 100%;
}
/*第一部分内容区*/
.swiper-item:nth-of-type(1) {
  height: 100%;
  background-color: #f5f5f5;
}
.swiper-item:nth-of-type(1) .proList {
  width: 100%;
  height: 100%;
}
.banner{
  width:100%;
  height:200px;
}
.bannerList{
  width:100%;
  height:200px;
}
/*第二部分内容区*/
.swiper-item:nth-of-type(2) {
  height:100%;
}
.jiawu_warp,.xiachu_warp,.jiaju_warp,.life_warp,.ruanzhuang_warp,.chuangpin_warp,.work_warp{
  width:100%;
  height:100%;
  display: flex;
  flex-direction: column;
  background-color: #efefef;
}
.jiawu_header{
  width:100%;
  height:auto;
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
}
.jiawu_header_li{
  width:25%;
  height:240rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.jiawu_header_li view{
  width:80%;
  flex: 1;
}
.jiawu_header_li view image{
  width:100%;
  height:100%;
}
.jiawu_header_li text{
  width:100%;
  flex: 1;
  line-height:90rpx;
  text-align: center;
  font-size: 32rpx;
}
.bannerimg{
  width:100%;
  height:300rpx;
}
.jiawu_con{
  flex: 1;
  width:100%;
  background-color: #efefef;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.jiawu_contit{
  display: block;
  width:96%;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
  font-size: 40rpx;
}
.jiawu_conList{
  width:96%;
  flex: 1;
}
.jiawu_con_li{
  width:100%;
  height:500rpx;
  margin-bottom: 20rpx;
}
.jiawu_con_li_img{
  width:100%;
  height:360rpx;
  position: relative;
}
.jiawu_con_li_img image{
  width:100%;
  height:100%;
}
.jiawu_con_li_img text{
  position: absolute;
  left:0;
  bottom: 0;
  width:100%;
  height:50rpx;
  line-height:50rpx;
  color:#fff;
  text-indent: 20rpx;
  background:rgba(0,0,0,0.2);
}
.jiawu_con_li_name{
  width:100%;
  height:70rpx;
  line-height:70rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}
.jiawu_con_li_name text{
  flex: 1;
  height:70rpx;
  line-height: 70rpx;
  font-size: 36rpx;
}
.jiawu_con_li_name view{
  width:140rpx;
  height:70rpx;
  display: flex;
}
.jiawu_con_li_name view text{
  flex: 1;
  height:70rpx;
  line-height:70rpx;
  font-size: 32rpx;
  color:#999;
  text-align: center;
}
.jiawu_con_li_name view image{
  margin-top:5rpx;
  width:60rpx;
  height:60rpx;
}
.jiawu_con_li_price{
  width:100%;
  height:70rpx;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
}
.jiawu_con_li_price view{
  flex: 1;
  height:70rpx;
}
.jiawu_con_li_price view text:nth-of-type(1){
  display: inline-block;
  width:100rpx;
  height:70rpx;
  line-height:70rpx;
}
.jiawu_con_li_price view text:nth-of-type(2){
  display: inline-block;
  width:50rpx;
  height:50rpx;
  color:#fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 50rpx;
  background-color: #CD060F;
  margin:10rpx;
}
.jiawu_con_li_price>text{
  width:180rpx;
  height:70rpx;
  line-height: 70rpx;
  font-size: 32rpx;
  text-align: right;
}






 .swiper-item:nth-of-type(3) {
  height:100%;
}
.swiper-item:nth-of-type(4) {
