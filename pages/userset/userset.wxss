/* pages/userset/userset.wxss */
page{
    width:100%;
    height:100%;
    display: flex;
}
.wrap{
    flex: 1;
    height:100%;
    background-color: #f5f5f5;
}
.wrap view{
    width:100%;
    height:100rpx;
    margin:10rpx auto;
    border-bottom: 1rpx solid #ccc;
    display: flex;
    justify-content: space-between;
    background-color: #fff;
}
.wrap view text{
    flex: 1;
    height:100rpx;
    line-height:100rpx;
    text-indent: 15rpx;
    font-size: 28rpx;
}
.wrap view image{
    width:48rpx;
    height:48rpx;
    margin:25rpx 15rpx;
}
.wrap button{
    width:90%;
    height:100rpx;
    margin:30rpx auto 10rpx;
    line-height:100rpx;
    color:#fff;
    font-size: 30rpx;
}



































