page{
    width:100%;
    height:100%;
    display: flex;
    flex-direction: column;
    background-color: #f5f5f5;
}
.header{
    width:100%;
    height:240rpx;
    background-color: #f66;
}
.registerCon{
    display: block;
    flex: 1;
    width:100%;
}
.registerCon  view{
    width:96%;
    height:120rpx;
    margin:0 2%;
    border-bottom: 2rpx solid #ccc;
    background-color: #fff;
}
.registerCon view label{
    float: left;
    width:200rpx;
    height:120rpx;
    line-height:120rpx;
    font-size: 34rpx;
    color:#333;
    text-indent: 20rpx;
}
.registerCon view input{
    float: left;
    height:120rpx;
    line-height:120rpx;
    text-indent: 10rpx;
    font-size: 30rpx;
}
.registerCon .randomtxt{
    position: relative;
    margin-bottom: 30rpx;
    border-bottom: none;
}
.randomtxt text{
    display: block;
    position: absolute;
    right:30rpx;
    top:20rpx;
    width:200rpx;
    height:80rpx;
    line-height:80rpx;
    text-align: center;
    background-color: #89be48;
    font-size: 30rpx;
    color: #fff; 
}
.registerCon button{
    width:96%;
    height:100rpx;
    margin:0 auto;
    border-radius: 0;
}

